import turtle
import time
import random
`
griffin = turtle.Turtle()
griffin.shape("turtle")
griffin.color("green")
griffin.speed(6)
griffin.goto(-200,0)`

racer = turtle.Turtle
racer.shape("turtle")
racer.color("blue")
racer.speed(6)
racer.goto(-200,0)

builder = turtle.Turtle()
builder.shape("turtle")
builder.color("black")
builder.speed(0)
builder.pendown()
builder.forward(300)
builder.backward(600)
builder.penup()



